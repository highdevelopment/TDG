<!doctype html>
<html>
	<head>
		<meta http-equiv="X-UA-Compatible" content="IE=Edge">
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
		<meta name="viewport" content="width=1200, initial-scale=1.0, maxium-scale=1.0, minimum-scale=1.0, user-scalable=yes" />
		<meta name="format-detection" content="telephone=no" />
		<!-- Favicon -->
		<link rel="shortcut icon" href="images/favicon.ico" type="image/x-icon" />
		<link rel="apple-touch-icon" href="images/apple-touch-icon.png">
		<link rel="stylesheet" href="../style/css/reset.css" media="all">
		<title>더 도어스 스크린 골프 1:1 문의</title>
		<!--[if lt IE 9]>
			<script src="http://ie7-js.googlecode.com/svn/version/2.1(beta4)/IE9.js" type="text/javascript"></script>
			<script src="http://ie7-js.googlecode.com/svn/version/2.1(beta4)/ie7-squish.js" type="text/javascript"></script>
			<script src="http://html5shim.googlecode.com/svn/trunk/html5.js" type="text/javascript"></script>
			<script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
		<![endif]-->
		<script type="text/javascript" src="../style/js/jquery-1.11.3.min.js"></script>
		<script type="text/javascript" src="../style/js/jquery.bxslider/jquery.bxslider.min.js"></script>
		<script type="text/javascript" src="../style/js/main.js"></script>
	</head>
	<body>

	<!--헤더-->
	{{>header}}
	<!--헤더-->

	<!--서브탑-->
	<div class="subTop">
		<div class="subTopInner">
			<h1>1:1 문의</h1>
			<div class="location">
				<a href="my-qamake">MY DOORS</a> > <span>1:1 문의</span>
			</div>
		</div>
	</div>
	<!--서브탑-->

	<!--바디-->
	<article>
		<div id="bodyWrap">
		<!--내용시작-->
		<div class="wrap835">


			<div class="qnaBtn"><a href="my-qamake">1:1 문의하기</a></div>
			<!--게시판 리스트-->
			<div class="bbsList">
				<table>
					<caption>1:1문의하기 목록</caption>
					<colgroup>
					</colgroup>
					<thead>
						<tr>
							<th>번호</th>
							<th>제목</th>
							<th>등록일</th>
							<th>처리여부</th>
						</tr>
					</thead>
					<tbody>
						{{#each questions}}
						<tr id="{{this.id}}">
							<td>{{this.index}}</td>
							<td class="left"><a href="#">{{this.title}}<a/></td>
							<td>{{this.time}}</td>
							<td class="{{this.statusClass}}">{{this.status}}</td>
						</tr>
						{{/each}}
					</tbody>
				</table>
				<!--게시판 리스트-->
			</div>

			<!--페이징-->
			<div class="paging">
				<a href="?search_type={{params.search_type}}&search={{params.search}}&page={{page.prevPage10}}" class="prev10 {{page.prev10_disable}}">이전10</a>
				<a href="?search_type={{params.search_type}}&search={{params.search}}&page={{page.prevPage}}" class="prev {{page.prev_disable}}">이전</a>
				{{#for 0 page.length 1}}
					{{#if (eq @root.page.pageIndex this)}}
						<a href="?search_type={{params.search_type}}&search={{params.search}}&page={{this}}" class="on">{{math this "+" 1}}</a>
					{{else}}
						<a href="?search_type={{params.search_type}}&search={{params.search}}&page={{this}}" class="">{{math this "+" 1}}</a>
					{{/if}}
				{{/for}}
				<a href="?search_type={{params.search_type}}&search={{params.search}}&page={{page.nextPage}}" class="next {{page.next_disable}}">다음</a>
				<a href="?search_type={{params.search_type}}&search={{params.search}}&page={{page.nextPage10}}" class="next10 {{page.next10_disable}}">다음10</a>
			</div>
			<!--페이징-->
			<br>

			 <!--게시판 검색-->
			<form id="form_search" action="/my-qalist">
				<div class="searchBbs">
					<select id="select_search_type" name="search_type" title="검색조건">
						<option value="0">제목</option>
						<option value="1">내용</option>
						{{!-- <option value="2">제목+내용</option> --}}
					</select>
					<input type="text" id="" name="search" value="{{params.search}}" title="검색어">
					<input type="submit" value="검색" title="검색">
					{{!-- <a href="?search_type={{params.search_type}}&search={{params.search}}&page={{page.pageIndex}}">검색</a> --}}
				</div>
			</form>
			 <!--게시판 검색-->

		
		</div>
		<!--내용끝-->
		</div>
	</article>
	<!--바디-->

	{{>footer}}
	
		<script>
			jQuery(document).ready(function() {
				var search_type = '{{params.search_type}}';
				$('#select_search_type').val(search_type);
		        $("table tbody tr").click(function(e){
		            var id = $(this).attr('id');
		            var url = "my-qadetail?id=" + id;
					$(location).attr('href',url);
					// window.location.replace(url);
		        });
			})
		</script>
	</body>
</html>
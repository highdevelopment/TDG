{
  "name": "GameLogs",
  "description": "유저별 게임로그",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "strictObjectIDCoercion": true
  },
  "properties": {
    "playerId": {
      "type": "string",
      "required": true,
      "description": "플레이어아이디 (비로그인가입자포함)"
    },
    "playerName": {
      "type": "string",
      "required": true,
      "description": "플레이어이름 (비로그인가입자포함)"
    },
    "game_holenum": {
      "type": "number",
      "required": true,
      "description": "9, 18 홀개수",
      "default": 18
    },
    "course": {
      "type": "string",
      "required": true,
      "description": "코스이름"
    },
    "scores": {
      "type": "object",
      "required": true,
      "description": "점수배렬",
      "default": "[]"
    },
    "score": {
      "type": "number",
      "required": true,
      "description": "점수(점수배렬의 합)",
      "default": 0
    },
    "putting_num": {
      "type": "object",
      "required": true,
      "description": "퍼팅수배렬",
      "default": "[]"
    },
    "rank": {
      "type": "number",
      "description": "랭킹",
      "default": 0
    },
    "holeIndex": {
      "type": "number",
      "required": true,
      "description": "마지막기록의 홀번호",
      "default": 0
    },
    "game_num": {
      "type": "number",
      "required": false,
      "description": "게임을 진행한 홀개수",
      "default": 0
    },
    "createdAt": {
      "type": "date",
      "required": true,
      "default": "$now"
    },
    "modifiedAt": {
      "type": "date",
      "required": true,
      "default": "$now"
    },
    "roomId": {
      "type": "string",
      "required": true,
      "description": "룸아이디"
    },
    "storeId": {
      "type": "string",
      "required": true,
      "description": "매장아이디"
    },
    "game_context": {
      "type": "object",
      "required": false,
      "description": "게임내용",
      "default": ""
    }
  },
  "validations": [],
  "relations": {
    "accounts": {
      "type": "belongsTo",
      "model": "Accounts",
      "foreignKey": ""
    },
    "game": {
      "type": "belongsTo",
      "model": "games",
      "foreignKey": ""
    },
    "motionVideo": {
      "type": "hasMany",
      "model": "MotionVideo",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "user",
      "permission": "ALLOW",
      "property": "getGameLog"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "manager",
      "permission": "ALLOW",
      "property": "getGameLog"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "registerGameLog"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "updateGameLog"
    }
  ],
  "methods": {}
}
